{
    "command": "register",
    "description": "Registers a package.",
    "usage": [
        "register <name> <url> [<options>]"
    ],
    "options": [
        {
            "shorthand":   "-f",
            "flag":        "--force",
            "description": "Bypasses confirmation. Bower login is still needed."
        },
        {
            "shorthand":   "-h",
            "flag":        "--help",
            "description": "Show this help message"
        }
    ]
}
